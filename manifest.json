{
  "manifest_version": 2,
  "name": "Spinblock",
  "version": "0.1.0",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "web_accessible_resources": [
    "template.html"
  ],
  "background": {
    "scripts": ["underscore-1.8.3.min.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-2.1.4.min.js", "content.js"],
      "run_at": "document_start"
    }
  ]
}
